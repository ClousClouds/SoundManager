{
    "name": "xpocketmp/sounds",
    "description": "A library for XPocketMP for handling Minecraft Bedrock Edition sounds",
    "type": "library",
    "license": "MIT",
    "require": {
        "pocketmine/math": "1.0.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.12.0 || ~2.1.0",
        "phpstan/phpstan-strict-rules": "^1.0.0 || ~2.0.1",
        "xpocketmc/xpocketmp": "dev-stable"
    },
    "autoload": {
        "psr-4": {
            "\\pocketmine\\world\\sound\\": "src/world/sound"
        }
    },
    "scripts": {
        "phpstan": "phpstan analyse"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
